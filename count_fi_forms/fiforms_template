###!
#define EXTMAXLEN ###%0^1###%

typedef char* FiFormExtArr[FORMCOUNT];

typedef enum FiFormId {
###!
    ###%1^0###%


} fiFormId;

FiFormExtArr fiFormExtArr = {
###!
    ###%1^1###%
};

typedef FiFormExtArr* fiFormExt;

